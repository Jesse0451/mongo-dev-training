// insert many 

db.movies.insertMany(
    [
        { "_id": "Jaws", "year": 1975 },
        { "_id": "Titanic", "year": 1997 },
        { "_id": "The Lion King", "year": 1994 }
    ],
    { ordered: false }
)
db.movies.find()


// the mongo shell is a js interpretor! which means all sorts of fun stuff.
for (i = 1; i <= 10000; i++) {
    db.stuff.insertOne({ "a": i })
}
db.stuff.find()
